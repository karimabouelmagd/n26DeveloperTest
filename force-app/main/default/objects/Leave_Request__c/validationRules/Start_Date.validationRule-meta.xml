<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_Date</fullName>
    <active>true</active>
    <errorConditionFormula>If( Start_Date__c &lt; Today(),TRUE,FALSE)</errorConditionFormula>
    <errorMessage>Start Date Cannot be in the Past</errorMessage>
</ValidationRule>
