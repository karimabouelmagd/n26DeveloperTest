<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date</fullName>
    <active>true</active>
    <errorConditionFormula>If( End_Date__c - Start_Date__c &gt; 21,TRUE,FALSE)</errorConditionFormula>
    <errorMessage>End Date Cannot be more than 3 weeks</errorMessage>
</ValidationRule>
